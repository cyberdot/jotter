@model Jotter.Core.Post.PostViewModel

<div class="post">
	<h1>@Model.frontmatter.title</h1>
	<span class="post-date">@Model.frontmatter.created.ToString(Model.config.dateFormat)</span>
	@Model.content
</div>

@if (Model.config.enable_disqus_comments) {
	@Jotter.Core.DisqusComments.render(Model.path, Model.config.disqus_url)	 
}

